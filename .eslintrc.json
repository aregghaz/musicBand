{
  "extends": [
    "next/core-web-vitals"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "off",
      {
        "semi": true,
        "singleQuote": true,
        "tabWidth": 2,
        "printWidth": 80,
        "trailingComma": "es5"
      }
    ]
  }
}
